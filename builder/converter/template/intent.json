{
  "name": {{ name }},
  "description": {{ description }},
  "slots": {{ slots }},
  {%if confirmationPrompt is not none %}
  "confirmationPrompt": {
    "maxAttempts": {{maxAttempts}},
    "messages": [
      {
        "content": {{confirmationPrompt}},
        "contentType": "PlainText"
      }
    ]
  },
  {% endif %}
  {%if rejectionStatement is not none %}
  "rejectionStatement": {
    "messages": [
      {
        "content": {{rejectionStatement}},
        "contentType": "PlainText"
      }
    ]
  },
  {% endif %}
  "sampleUtterances": [
    {% for value in sampleUtterances %}
    {{ value }}
    {%- if not loop.last -%},
    {%- endif %}
    {% endfor %}
  ],
  "dialogCodeHook":{
      "uri": {{lexDispatcher}},
      "messageVersion": "1.0"
  },
  "fulfillmentActivity":
    {
      "type": "CodeHook",
      "codeHook": {
            "uri": {{lexDispatcher}},
            "messageVersion": "1.0"
       }
    }
}